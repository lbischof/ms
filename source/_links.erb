<div class="container links">
    <h1><%= pdata.title %></h1>
    <% pdata.links.each do |header| %>
        <h2><%= header.name %></h2>
        <% header.links.each do |link| %>
            <a href="<%= link.url %>" target="_blank"><%= link.name %></a><br>
        <% end %>
    <% end %>
    <p class="disclaimer">
    We are not responsible for the content of external links.
    </p>
</div>
